{
  "name": "StudyAtHome",
  "version": "1.0.0",
  "description": "Ouchi De Jikanwari Web Version",
  "author": "Nekoya3",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3000 NODE_ENV=development nuxt-ts",
    "build": "yarn prebuild && nuxt-ts build",
    "prebuild": "cross-env NODE_ENV=development node pre_build_script/swEnvBuild.js",
    "test:unit": "jest --config jest.config.js",
    "start": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=3000 nuxt-ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .eslintignore .",
    "clean": "rimraf .nuxt dist",
    "copy": "yarn copy:client && yarn copy:server",
    "copy:client": "cpx -C \"src/static/**\" dist/client/ && cpx -C \".nuxt/dist/**\" dist/client/assets/",
    "copy:server": "cpx -C \"functions/**\" dist/server && cpx \".nuxt/**\" dist/server/",
    "install:functions": "yarn install --cwd dist/server",
    "firebase-emulator": "yarn clean && yarn lint && yarn build && yarn copy && yarn install:functions && firebase emulators:start"
  },
  "lint-staged": {
    "*.{js,ts,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.3",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.0.14",
    "cookieparser": "^0.1.0",
    "core-js": "3.6.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.14.0",
    "dayjs": "^1.8.26",
    "express": "^4.17.1",
    "firebase": "7.15.5",
    "firebase-admin": "^8.12.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "nuxt": "^2.13.2",
    "nuxt-property-decorator": "^2.7.2",
    "nuxt-svg-loader": "^1.2.0",
    "nuxt-webfontloader": "^1.1.0",
    "uuid": "^8.2.0",
    "vue-property-decorator": "^8.4.2",
    "vuex-class-component": "^2.3.3",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/jest": "^26.0.3",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "cpx": "^1.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^26.1.1",
    "vue-jest": "^3.0.5"
  }
}
